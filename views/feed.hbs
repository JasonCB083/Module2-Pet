{{>menu}}

<div class='feed-container'>
  <h1>The pets!</h1>

  {{#each pets}}
  <p>Name: {{this.petName}}</p>
  <p>Kind: {{this.type}}</p>
  <p>Size: {{this.size}}</p>
  <a href="pet/{{this._id}}"><img src="{{this.picture}}"></a>
  {{/each}}

</div>

<div class="feed">
	{{#each pets}}
		{{> petcard}}
	{{/each}}
</div>
